# Asterisk VoIP Environment Configuration

# Timezone
TZ=Europe/Paris

# Logging level (0-8, higher = more verbose)
SYSLOG_LEVEL=4

# TLS certificate settings
TLS_CERTDAYS=365
TLS_KEYBITS=2048

# FreePBX database (if using freepbx profile)
DB_PASSWORD=asterisk-secure-password
DB_ROOT_PASSWORD=root-secure-password
